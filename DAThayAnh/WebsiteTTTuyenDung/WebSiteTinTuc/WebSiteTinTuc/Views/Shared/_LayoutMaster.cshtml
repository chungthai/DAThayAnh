                                        {
                                            <li><a class="login-window" href="~/Account/DangNhap">Đăng nhập</a></li>
                                            <li><a href="~/Account/DangKy">Đăng kí</a></li>
                                        }

                                        <li>
                                            <a href="#" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">
                                                <span class="fa fa-search"></span>
                                            </a>
                                            <ul class="dropdown-menu">
                                                <li>
                                                    @using (Html.BeginForm("KetQuaTimKiem", "Home"))
